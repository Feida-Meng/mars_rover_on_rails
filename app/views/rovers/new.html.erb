<div class="new_rover">
  <h1>Add a new rover</h1>

  <%= form_tag "/rovers" do%>
    <p>
      <%= label_tag :current_position%> <br>
      <%= text_field_tag :current_position%>
    </p>

    <p>
      <%=label_tag :move%> <br>
      <%=text_field_tag :move%>
    </p>

    <p>
      <%= submit_tag "Save" %>
    </p>
  <%end%>

<div/>
